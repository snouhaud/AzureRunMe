    <Certificates>
      <Certificate name="Microsoft.WindowsAzure.Plugins.RemoteAccess.PasswordEncryption" thumbprint="E16E8C182ECE44B5B1E3742955A5D022BB6F30B2" thumbprintAlgorithm="sha1" />
